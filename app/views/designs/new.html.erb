<h2>Create New Design</h2>

<%= render partial: 'errors/errors', locals: {model: @design} %>

<%= form_with model: [@collage, @design] do |f| %> 
<%= f.label :title %>  
<%= f.text_field :title %><br>
<%= f.label :description %>
<%= f.text_field :description %><br>
<%= f.label :design_url %>
<%= f.text_field :design_url %><br>

<%= fields_for_helper(f) %>


<%= f.submit 'Create Design'%>
<% end %>
